{
  "name": "siasky-cli",
  "version": "1.0.2",
  "description": "CLI for Skynet with inline-source",
  "main": "dist/index.js",
  "bin": {
    "siasky-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "babel src -d dist -s",
    "format": "prettier --write './{src,test}/**/*.{js,json,css}'",
    "prebuild": "mkdirp dist",
    "lint": "eslint src",
    "pretest": "mkdirp test/out",
    "test": "mocha --timeout 5000",
    "prepare": "npm run build",
    "release": "npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bakaoh/siasky-cli.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2"
  },
  "bugs": {
    "url": "https://github.com/bakaoh/siasky-cli"
  },
  "homepage": "https://github.com/bakaoh/siasky-cli",
  "keywords": [
    "skynet",
    "cli"
  ],
  "engines": {
    "node": ">=8.3.0"
  },
  "author": "Bakaoh <tatattai@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^3.0.0",
    "inline-source": "^7.1.0",
    "yargs": "^14.2.0"
  },
  "prettier": {
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "strict",
    "printWidth": 80,
    "singleQuote": true
  }
}